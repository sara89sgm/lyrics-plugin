{
  "name": "BBC Radio LYRICS",
  "description": "Lyrics for BBC Radio",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "http://nhp-service.test.api.bbci.co.uk/realtime/bbc_radio_two", "http://http://api.musixmatch.com/ws/1.1/*"
  ],
  "browser_action": {
      "default_title": "BBC Radio",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 3,

  "permissions": ["http://localhost/*",  "notifications", "*://*/*"],
    "web_accessible_resources": [
        "assets/js/require.js",
                "assets/js/main.js",
                "assets/js/jquery-3.0.0.js",
                "assets/js/on-air-now.js",
                "assets/js/on-air.js",
                "assets/js/on-air-paddles.js",
                "assets/js/on-air-schedule.js",
                "assets/js/pollingv2.js"
    ]
}
